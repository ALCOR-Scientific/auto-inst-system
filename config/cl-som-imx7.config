# Configuration file for CL-SOM-iMX7

[ Parameters ]
tty=ttymxc0,115200
rdaddr=0x82000000
loadaddr=0x80800000
fdtaddr=0x83000000
target_media=mmcblk2 mtd
first_boot_mmcblk2=run emmcboot
first_boot_mtd=run nandboot
dtb_file=imx7d-sbc-imx7.dtb
mtd=4:0:3:0:5:0:rootfs
unlock_dev=/dev/mtd2
[ fw_env.config ]
# MTD device name       Device offset   Env. size       Flash sector size
/dev/mtd1               0               0x2000          0x2000
[ eof ]
Should remain last section
