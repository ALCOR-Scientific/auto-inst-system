# Configuration file for CL-SOM-iMX6UL

[ Parameters ]
tty=ttymxc2,115200
rdaddr=0x81000000
fdtaddr=0x83000000
target_media=mmcblk1
first_boot_mmcblk1=setenv mmcdev 1; setenv mmcroot /dev/mmcblk1p2 rootwait rw; mmc dev ${mmcdev}; mmc rescan; run loadimage; run mmcboot
unlock_dev=/dev/mtd2
[ fw_env.config ]
# MTD name      Device offset   Env. size       Flash sector size
/dev/mtd1       0x0000          0x2000          0x2000
[ eof ]
Should remain last section
