::sysinit:/etc/init.d/rcS
::sysinit:/etc/init.d/install_sd.sh

# Stuff to do when restarting the init process
::restart:/sbin/init

# Stuff to do before rebooting
::ctrlaltdel:/sbin/reboot
::shutdown:/bin/umount -a -r
::shutdown:/sbin/swapoff -a

# Console
::respawn:/sbin/getty -l /usr/bin/autologin 38400 tty1 
::respawn:/sbin/getty -l /usr/bin/autologin -L 115200 ttyO0 vt100
